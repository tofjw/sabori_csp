% array_var_bool_element test: array[index] = result with bool array
var 1..3: idx :: output_var;
var 0..1: b1 :: output_var;
var 0..1: b2 :: output_var;
var 0..1: b3 :: output_var;
var 0..1: result :: output_var;
constraint int_eq(idx, 2);
constraint int_eq(b1, 0);
constraint int_eq(b2, 1);
constraint int_eq(b3, 0);
constraint array_var_bool_element(idx, [b1, b2, b3], result);
solve satisfy;
